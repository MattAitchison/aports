pkgname=dos-keys
pkgver=1.1
pkgrel=0
pkgdesc="Public keys for D/os packages"
url="http://doslinux.org"
arch="noarch"
license="GPL"
depends=""
makedepends=""
install=""
subpackages=""
replaces="dos-base"
source="smoku-5425c590.rsa.pub"

build() {
	return 0
}

package() {
	# copy keys for repos
	mkdir -p "$pkgdir"/System/Settings/apk/keys
	install -m644 "$srcdir"/*.pub \
		"$pkgdir"/System/Settings/apk/keys/ || return 1
}

